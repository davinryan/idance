{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": [false, {"multiline": "never", "singleline": "never"}],
    "eofline": false
  }
}
